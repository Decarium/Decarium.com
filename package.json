{
  "name": "Decarium.com",
  "version": "0.1.0",
  "private": true,
  "description": "Deacarium's website",
  "scripts": {
    "start": "webpack-dashboard -- webpack-dev-server --config webpack.config.dev.js",
    "clean:dist": "rm -rf dist",
    "build": "BABEL_ENV=production NODE_ENV=production webpack",
    "deploy": "yarn clean:dist && yarn build && deploy/deploy.sh",
    "lint:js": "eslint src",
    "lint:css": "stylelint \"src/**/*.css\"",
    "lint": "npm run lint:js && npm run lint:css",
    "test": "jest",
    "test:watch": "npm run test -- --reporter min --watch",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov",
    "flow": "flow; test $? -eq 0 -o $? -eq 2"
  },
  "dependencies": {
    "autoprefixer": "^7.1.6",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.0.0",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-istanbul": "^3.0.0",
    "babel-plugin-react-intl": "^2.2.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.16.0",
    "babel-runtime": "^6.20.0",
    "brotli-webpack-plugin": "^0.5.0",
    "case-sensitive-paths-webpack-plugin": "^2.1.1",
    "classnames": "^2.2.5",
    "compression-webpack-plugin": "^1.0.1",
    "copy-webpack-plugin": "^4.2.0",
    "css-loader": "^0.28.7",
    "cssnano": "^3.10.0",
    "eslint": "^3.12.2",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "v2.2.3",
    "eslint-plugin-react": "^6.8.0",
    "extract-text-webpack-plugin": "2.0.0-rc.3",
    "fingerprintjs2": "^1.4.1",
    "html-webpack-plugin": "^2.24.1",
    "istanbul": "1.0.0-alpha.2",
    "jest": "^19.0.2",
    "jsdom": "^9.8.3",
    "mocha": "^3.2.0",
    "normalize.css": "^5.0.0",
    "nyc": "next",
    "offline-plugin": "^4.5.3",
    "open-color": "^1.4.0",
    "postcss-loader": "^1.2.1",
    "postcss-reporter": "^3.0.0",
    "postcss-smart-import": "^0.6.3",
    "precss": "^1.4.0",
    "prop-types": "^15.6.0",
    "react": "^16.1.0",
    "react-addons-css-transition-group": "^15.4.1",
    "react-dom": "^16.1.0",
    "react-ga": "^2.3.5",
    "react-heatmapjs": "1.2.1",
    "react-helmet": "^5.2.0",
    "react-hot-loader": "^3.1.2",
    "react-intl": "^2.2.0",
    "react-router-dom": "^4.2.2",
    "react-test-renderer": "^16.1.1",
    "react-user-focus": "1.3.5",
    "sitemap-webpack-plugin": "^0.5.1",
    "source-map-loader": "^0.2.3",
    "style-loader": "^0.19.0",
    "stylelint": "^8.2.0",
    "stylelint-config-standard": "^17.0.0",
    "webpack": "^3.8.1",
    "webpack-dashboard": "^1.0.2",
    "webpack-dev-server": "^2.9.4"
  },
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "src/**/*.spec.js"
    ],
    "require": [
      "babel-register"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "all": true,
    "sourceMap": false,
    "instrument": false
  },
  "stylelint": {
    "extends": "stylelint-config-standard"
  }
}
